{
  "name": "pucci/laravel-helpers",
  "description": "Pacchetto Laravel per salvare e gestire file",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Pucci\\LaravelHelpers\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/filesystem": "^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pucci\\LaravelHelpers\\LaravelHelpersServiceProvider"
      ],
      "aliases": {
        "FileSaver": "Pucci\\LaravelHelpers\\Facades\\FileSaver"
      }
    }
  },
  "repositories": [
        {
            "type": "path",
            "url": "../laravel-helpers"
        }
    ]
}
